<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jolsensei.quizparty">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/CustomAppCompat">
        <activity
            android:name=".Views.paginaPrincipal"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Views.paginaListadoQuizs"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Views.DialogDificultad"
            android:theme="@style/MyTheme" />
        <activity android:name=".Views.jugandoQuiz"></activity>
    </application>

</manifest>